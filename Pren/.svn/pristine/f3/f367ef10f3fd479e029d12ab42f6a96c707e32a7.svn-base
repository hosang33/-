<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="ko">
<head>
  	<title></title>
  	<meta charset="utf-8">
  	<meta name="viewport" content="width=device-width, initial-scale=1">
  	<style type="text/css">
  		img {
  			box-sizing: border-box;
			width: 365px;
  			max-width: 365px;
  			height: 276px;
  			max-height: 276px;
  		}
  	</style>
</head>
<body>
<div style="margin: 10px;">
	<%
		String imagename = request.getParameter("imagename");
	%>
	
	<img src="../../resources/images/review/<%=imagename %>" id="image-box" class="img-thumbnail" onclick="closePopup()">
	
</div>
<script type="text/javascript">
	function closePopup() {
		window.close();
	}
</script>
</body>
</html>
